{
  "name": "xss-sample-app",
  "version": "1.0.0",
  "description": "A sample environment to test XSS vulnerability.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "start-attacker": "node attacker-server.js",
    "client": "cd client && vite",
    "build": "cd client && vite build",
    "dev": "concurrently \"npm start\" \"npm run attacker\" \"cd client && npm run dev\"",
    "seed": "node scripts/seedMovies.js",
    "seed:watch": "nodemon scripts/seedMovies.js",
    "test": "jest --detectOpenHandles",
    "test:auth": "jest tests/auth.test.js --detectOpenHandles",
    "test:integration": "jest tests/integration.test.js --detectOpenHandles --runInBand"
  },
  "keywords": [],
  "author": "Andrea Chiarelli",
  "license": "ISC",
  "devDependencies": {
    "axios": "^1.13.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.3.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-loader": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.2.1",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "moment": "^2.30.1",
    "mongoose": "^8.0.0",
    "nodemon": "^3.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "vite": "^7.1.12",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
